{
	"name": "branium-messages",
	"version": "0.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"rimraf": "./node_modules/rimraf/bin.js",
		"clean": "rimraf dist",
		"start": "node  --experimental-specifier-resolution=node dist/main/server.js",
		"tsc": "tsc",
		"tsc-alias": "tsc-alias",
		"dev": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn --exit-child src/main/server.ts",
		"build": "npm-run-all clean tsc tsc-alias",
		"prepare": "husky install",
		"db:migration:up": "tsx ./src/infra/db/kysely/commands/runMigrations.ts",
		"db:migration:down": "tsx ./src/infra/db/kysely/commands/downMigration.ts",
		"make:usecase": "plop --plopfile ./.plop/usecase.mjs",
		"make:dto": "plop --plopfile ./.plop/dto.mjs",
		"make:validator": "plop --plopfile ./.plop/validator.mjs",
		"make:repository": "plop --plopfile ./.plop/repository.mjs"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/busboy": "^1.5.3",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.10.4",
		"@types/nodemailer": "^6.4.14",
		"@types/pg": "^8.10.9",
		"@types/typescript": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^1.0.1",
		"plop": "^4.0.0",
		"pretier": "^0.0.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.470.0",
		"@aws-sdk/s3-request-presigner": "^3.470.0",
		"@sendgrid/mail": "^8.1.0",
		"bcryptjs": "^2.4.3",
		"busboy": "^1.6.0",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.2",
		"kysely": "^0.26.3",
		"nodemailer": "^6.9.7",
		"npm-run-all": "^4.1.5",
		"pg": "^8.11.3",
		"postgres": "^3.4.3",
		"rimraf": "^5.0.5",
		"tsc-alias": "^1.8.8",
		"tsx": "^4.6.2",
		"zod": "^3.22.4",
		"zod-error": "^1.5.0"
	}
}
